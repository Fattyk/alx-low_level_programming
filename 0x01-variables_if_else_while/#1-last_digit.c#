#include <stdlib.h>
#include <time.h>
#include (stdio.h)

/**
 * main - Entry Point
 *
 * Return: Always 0 (Success)
 */

int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	int k = (n % 10);
	if (k > 5)
	{
		printf("Last digit of %d is %d and is greater than 5\n", n, k);
	}
	else if (k == 0)
	{
		prinf("Last digit of %d is %d and is 0\n", n, k);
	}
	else if ((k < 6) && (k != 0))
	{
		prinf("Last digit of %d is %d \n", n, k);
	}
	return (0);
}
